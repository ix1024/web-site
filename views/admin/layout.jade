				iframe(src="", frameborder="0",class="tab-win-iframe",id="",tab-win-iframe="")
			block content
		script(src="/javascripts/src/lib/require.js")
		script(src="/javascripts/src/config/requirejs-config.js")
		script(src="/javascripts/src/admin.js")
		block scripts
