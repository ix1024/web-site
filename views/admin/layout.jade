doctype html
html
	head
		title= title
		link(rel='stylesheet', href='/stylesheets/admin.css')
	body
					
		div(class="admin-aside")
			div(class="admin-logo")
				a(href="/")
					img(src="/images/logo.png", alt="")
				//span=title
			include _aside
		div(class="admin-content")
			div(class="admin-header clearfix")
				div(class="fl")					
				div(class="fr")
